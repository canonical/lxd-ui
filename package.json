{
  "name": "lxd-ui",
  "version": "0.0.1",
  "author": "Canonical Webteam",
  "license": "LGPL-3.0-only",
  "scripts": {
    "clean": "rm -rf node_modules yarn-error.log css static/css *.log *.sqlite _site/ build/ .jekyll-metadata .bundle",
    "watch": "watch -p 'static/sass/**/*.scss' -p 'static/js/**/*' -c 'yarn run build'",
    "build-js": "webpack",
    "build-css": "sass static/sass/styles.scss build/css/styles.css --load-path=node_modules --style=compressed && postcss --use autoprefixer --replace 'build/css/**/*.css' --no-map",
    "build": "yarn run build-js && yarn run build-css",
    "format-js": "prettier 'static/js/**/*.{js,json,jsx}' --write",
    "lint-scss": "stylelint 'static/**/*.scss'",
    "lint-js": "eslint 'static/js/**/*.{js,json,jsx}' && prettier 'static/js/**/*.{js,json,jsx}' --check",
    "start": "yarn run build && concurrently --kill-others --raw 'yarn run watch' 'yarn serve'",
    "serve": "serve -s build"
  },
  "dependencies": {
    "@canonical/react-components": "0.37.6",
    "formik": "2.2.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "5.0.1",
    "serve": "14.0.1",
    "vanilla-framework": "3.8.0",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@babel/core": "7.17.2",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "autoprefixer": "9.8.8",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.3",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "concurrently": "5.3.0",
    "copy-webpack-plugin": "9.1.0",
    "css-loader": "5.2.7",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-react": "7.28.0",
    "postcss": "8.4.14",
    "postcss-cli": "8.3.1",
    "prettier": "2.5.1",
    "prop-types": "15.8.1",
    "sass": "1.49.7",
    "sass-loader": "10.2.1",
    "style-loader": "2.0.0",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "stylelint-prettier": "1.2.0",
    "stylelint-scss": "3.21.0",
    "watch-cli": "0.2.3",
    "webpack": "5.74.0",
    "webpack-cli": "3.3.12"
  }
}
